        "configuration": {
          "jsScript": "function nextNodes(metadata, msg) {\n    var results = []; // tableau des noms des prochains noeuds\n   \n    if (msg.values !== undefined && msg.values.poire0 === true && msg.values.poire1 === true && msg.values.poire2 === true && msg.values.poire3 === true) {\n         results.push('Cuve pleine');\n    }\n    if (msg.values !== undefined && msg.values.poire0 === false && msg.values.poire1 === false && msg.values.poire2 === false && msg.values.poire3 === false) {\n         results.push('Cuve vide');\n    }\n    if (msg.values !== undefined && metadata.bugTests !== \" \" ) {\n         results.push('Dysfonctionnement');\n    }\n    if (msg.values !== undefined && parseInt(metadata.pourcentage) < msg.values.pourcentage) {\n         results.push('Remplissage');\n    }\n    if (msg.values !== undefined && msg.values !== undefined && parseInt(metadata.pourcentage) > msg.values.pourcentage) {\n         results.push('Vidage');\n    }\n    if (msg.lowBattery === true) {\n        results.push('Batterie faible');\n    }\n    if (msg.signalValue === \"Mauvais\") {\n        results.push('Signal faible');\n    }\n    if (msg.indicator === \"OFF\") {\n        results.push('Capteur inactif');\n    }\n    return results;\n}\n\nreturn nextNodes(metadata, msg);\n\n// var result = false; if(test % 3(attributes) === 0 && test !== 0) { result = true; }"
