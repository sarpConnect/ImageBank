        },
        "type": "org.thingsboard.rule.engine.filter.TbJsSwitchNode",
        "name": "Check Alarms",
        "debugMode": true,
        "configuration": {
          "jsScript": "function nextNodes(metadata, msg) {\n    var results = []; // tableau des noms des prochains noeuds\n    if(typeof(msg.values) !== \"undefined\" && typeof(metadata.ss_customAlarm) !== \"undefined\") {\n        if(metadata.ss_customAlarm.trim() === \"\" || parseInt(metadata.ss_customAlarm) === 0) {\n            if (msg.values.total === 1) {\n                results.push('Infestation probable');\n            } else if (msg.values !== undefined && msg.values.total === 11) {\n                results.push('Infestation avérée');\n            } else if (msg.values !== undefined && msg.values.total === 21) {\n                results.push('Infestation critique');\n            }\n        } else if(msg.values.total !== 0 && msg.values.total % parseInt(metadata.ss_customAlarm) === 0) {\n            results.push('Alarme personnalisée');\n        }\n    }\n    if (msg.indicator === \"OFF\") {\n        results.push('Capteur inactif');\n    }\n    if (msg.battery < 20) {\n        results.push('Batterie faible');\n    }\n    if (msg.signal < 20) {\n        results.push('Signal faible');\n    }\n    if (msg.msgType === \"connectionStatus\" && msg.connection === \"OFFLINE\"){\n        results.push('Gateway OFFLINE');\n    }\n    return results;\n}\n\nreturn nextNodes(metadata, msg);"
        }
